    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1621.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "mongodb": "^6.5.0",
